#include<iostream>
#include<stdio.h>
#include<conio.h>
#include<math.h>
#include<cstring>
#include<format>
using namespace std;
class FLOAT1
{
	float b;
	public:
		FLOAT1(float b)
		{
			this->b=b;
		}
		float operator+(FLOAT1 F)
		{
			return(b+F.b);
		}
		float operator-(FLOAT1 F)
		{
			return(b-F.b);
		}
		float operator*(FLOAT1 F)
		{
			return(b*F.b);
		}
		float operator/(FLOAT1 F)
		{
			return(b/F.b);
		}
		  		
};
int main()
{
	FLOAT1 num1(83.87),num2(30.72);
	cout<<"\n\n Overloading of Operator + : "<<num1+num2;
	cout<<"\n\n Overloading of Operator - : "<<num1-num2;
	cout<<"\n\n Overloading of Operator * : "<<num1*num2;
	cout<<"\n\n Overloading of Operator / : "<<num1/num2;
	cout<<"\n\n Overloading of Operator / : "<<num1%num2;
return 0;
}
